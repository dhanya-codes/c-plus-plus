#include<iostream>
using namespace std;
void fun(''int n,int arr[])
{
	cout<<"enter array elements:"<<endl;
	for(int i=0;i<n;i++)
	{
	   cin>>arr[i];	
	}
}
void sort(int n,int arr[])
{
	for(int i=0;i<n-1;i++)
	{
		for(int j=0;j<n-i-1;j++)
		{
			if(arr[j]>arr[j+1])
			{
				int temp=arr[j];
				arr[j]=arr[j+1];
				arr[j+1]=temp;
			}
		}
	}
}
void display(int n,int arr[])
{
	sort(n,arr);
	cout<<"The array elements are:"<<endl;
	for(int i=0;i<n;i++)
	{
		if(i==0||arr[i]!=arr[i-1])
		{
		cout<<arr[i]<<" ";
	}
	}
}
 
int main()
{
	int n;
	cout<<"Enter the number of array elements:";
	cin>>n;
	int arr[100];
	fun(n,arr);
	sort(n,arr);
	display(n,arr);
}
